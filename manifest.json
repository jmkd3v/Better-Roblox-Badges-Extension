{
  "name": "Better Roblox Badges Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Improves Roblox Badges.",
  "homepage_url": "http://jmk.gg",
  "content_scripts": [{
    "js": ["inject.js"],
    "css": ["style.css"],
    "matches": ["*://*.roblox.com/*"]
  }],
  "web_accessible_resources": ["script.js", "style.css"],
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
		"*://*.roblox.com/*"
  ]
}